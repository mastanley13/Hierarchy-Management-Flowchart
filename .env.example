VITE_SURELC_BASE=https://surelc.surancebay.com/sbweb/ws
## Dual MRFG Admin Accounts (preferred)
VITE_SURELC_USER_EQUITA=your_equita_username
VITE_SURELC_PASS_EQUITA=your_equita_password
VITE_SURELC_USER_QUILITY=your_quility_username
VITE_SURELC_PASS_QUILITY=your_quility_password

## Legacy single-account fallback (optional)
VITE_SURELC_USER=your_agency_username
VITE_SURELC_PASS=your_agency_password
VITE_FIRM_ID_EQUITA=your_equita_firm_id
VITE_FIRM_ID_QUILITY=your_quility_firm_id
# Legacy compatibility if needed
VITE_FIRM_ID=your_legacy_firm_id
VITE_INITIAL_SNAPSHOT_DATE=2000-01-01T00:00:00Z
VITE_PAGE_LIMIT=500
 
# Optional: separate carrier credentials for carrier-only endpoints (CSV reports, uploads)
# If not set, the app will fall back to the agency credentials above.
VITE_CARRIER_USER=your_carrier_username
VITE_CARRIER_PASS=your_carrier_password

# Optional: call /firm/{id} to fetch firm details (some roles return 404)
# Set to 'false' to skip this call and rely on inferred name only.
VITE_FETCH_FIRM_DETAILS=true

# HighLevel / LeadConnector configuration
# Base API URL
HL_API_BASE=https://services.leadconnectorhq.com
# Location-scoped operations
HL_LOCATION_ID=nEEiHT9n7OPxFnBZIycg
# Custom field IDs (discovered via /locations/{locationId}/customFields)
HL_NPN_FIELD_ID=AlXLQ8VFgdbDMzGJ1uOR
HL_UPLINE_CODE_EQUITA_FIELD_ID=GM3pByQqhumadxL42TW4
HL_UPLINE_CODE_QUILITY_FIELD_ID=OlclNe3DpHXj2MVIJJJM
# Contact field used by the "Save upline" UI. Optional: if omitted, the app resolves it via /locations/{locationId}/customFields.
HL_UPLINE_PRODUCER_FIELD_ID=TobxJaosIfFMoD1q40dN
# Private Integration token (do NOT commit real value)
HL_PRIVATE_API_KEY=pit-1245ae68-1787-4106-95a8-bf079a78f64b
# Onboarding pipeline (used to derive "Upline Highest Stage" from the opportunity stage)
# If you don't know the ID, set the name (default: "Onboarding — Agent")
HL_ONBOARDING_PIPELINE_NAME=Onboarding — Agent
# Optional: prefer exact pipeline id match
# HL_ONBOARDING_PIPELINE_ID=your_pipeline_id
# Root hierarchy anchor (used when the upline cannot be resolved).
# If multiple contacts share the same NPN, set HL_ROOT_CONTACT_ID or HL_ROOT_CONTACT_EMAIL to break ties.
HL_ROOT_UPLINE_NPN=18550335
# HL_ROOT_CONTACT_ID=your_ghl_contact_id
# HL_ROOT_CONTACT_EMAIL=moe@example.com
# Optional tuning
HL_PAGE_SIZE=100
HL_RATE_LIMIT_PER_MIN=50
# Optional: exclude likely test contacts (for example, names containing "test")
# from upline parent-candidate matching. Contacts are still visible in the tree.
HL_UPLINE_EXCLUDE_TEST_CANDIDATES=0
# Optional precedence for conflicting upline sources: equita|quility
HL_UPLINE_PREFERRED_SOURCE=equita
