VITE_SURELC_BASE=https://surelc.surancebay.com/sbweb/ws
## Dual MRFG Admin Accounts (preferred)
VITE_SURELC_USER_EQUITA=your_equita_username
VITE_SURELC_PASS_EQUITA=your_equita_password
VITE_SURELC_USER_QUILITY=your_quility_username
VITE_SURELC_PASS_QUILITY=your_quility_password

## Legacy single-account fallback (optional)
VITE_SURELC_USER=your_agency_username
VITE_SURELC_PASS=your_agency_password
VITE_FIRM_ID_EQUITA=your_equita_firm_id
VITE_FIRM_ID_QUILITY=your_quility_firm_id
# Legacy compatibility if needed
VITE_FIRM_ID=your_legacy_firm_id
VITE_INITIAL_SNAPSHOT_DATE=2000-01-01T00:00:00Z
VITE_PAGE_LIMIT=500
 
# Optional: separate carrier credentials for carrier-only endpoints (CSV reports, uploads)
# If not set, the app will fall back to the agency credentials above.
VITE_CARRIER_USER=your_carrier_username
VITE_CARRIER_PASS=your_carrier_password

# Optional: call /firm/{id} to fetch firm details (some roles return 404)
# Set to 'false' to skip this call and rely on inferred name only.
VITE_FETCH_FIRM_DETAILS=true
